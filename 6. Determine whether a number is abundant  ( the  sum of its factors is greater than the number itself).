#include<stdio.h>
int main()
{
    int n, i, sum=0;
    printf("Enter any number :");
    scanf("%d",&n);
    for(i=1; i<n; i++)
    {
        if (n%i==0)
        {
            sum=sum+i;
        }
    }
    if(sum>n)
         printf("%d is an Abundant Number.",n);
    else 
         printf("%d is not an Abundant Number.",n);
   return 0;     
}
